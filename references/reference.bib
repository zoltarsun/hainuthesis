@article{cref3,
  title={植物抗冷性分子生物学研究进展},
  author={李美茹 and 刘鸿先},
  journal={热带亚热带植物学报},
  volume={8},
  number={1},
  pages={70-80},
  year={2000},
 keywords={植物抗冷性;膜脂;冷诱导蛋白;细胞抗氧化能力;钙信号系统},
 abstract={篇首: 温度是植物生长的必要条件,然而低温却是限制作物生产的重要因素,为此,各国政府及研究部门一直都把植物低温适应性问题作为一个重要的研究课题.按照低温 的不同程度,植物的低温伤害可分为冷害(chilling injury;零上低温对植物的伤害)和冻害(freezing injury;零下低温对植物的伤害)两大类.},
}
